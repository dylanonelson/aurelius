{
  "rules": {
    "logs": {
      "$uid": {
        ".indexOn": "date",
        ".write": "$uid === auth.uid",
        ".read": "$uid === auth.uid",
        "$logId": {
          "date": {
            ".validate": "newData.isString() && newData.val().matches(/^2\\d{3}-\\d{2}-\\d{2}$/)"
          }
        }
      }
    },
    "logTypes": {
      "$uid": {
        ".write": "$uid === auth.uid",
        ".read": "$uid === auth.uid"
      }
    },
    "benchmarks": {
      "$uid": {
        ".indexOn": "date",
        ".write": "$uid === auth.uid",
        ".read": "$uid === auth.uid"
      }
    }
    "benchmarkTypes": {
      "$uid": {
        ".write": "$uid === auth.uid",
        ".read": "$uid === auth.uid"
      }
    }
  }
}
